{
  "LambdaFunctionConfigurations": [
    {
      "Id": "TriggerOracleUpgradeFactoryOnMetrics",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:448792658038:function:oracle-upgrade-factory-trigger-start-sfn",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "runs/"
            },
            {
              "Name": "suffix",
              "Value": "/00-metadata/metrics.json"
            }
          ]
        }
      }
    }
  ]
}
